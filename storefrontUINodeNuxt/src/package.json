{
	"name": "nuxt3",
	"version": "0.2.0",
	"description": "a DeepWeb starter template or boilerplate with a lot of useful features. Nuxt3 + Tailwindcss 3",
	"repository": {
		"type": "git",
		"url": "git://github.com/vasilistotskas/grooveShop/storefrontUINodeNuxt"
	},
	"author": "vasilistotskas",
	"license": "MIT",
	"scripts": {
		"build": "nuxt build",
		"dev": "nuxt dev",
		"generate": "nuxt generate",
		"preview": "nuxt preview",
		"start": "node .output/server/index.mjs",
		"serve": "serve dist/",
		"lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
		"lint:fix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
		"format": "prettier --check --ignore-path .gitignore .",
		"format:fix": "prettier --write --ignore-path .gitignore .",
		"clean": "rm -rf .nuxt dist .output",
		"generate:locales": "node tools/translator.js ./locales en.yml",
		"dts": "vue-tsc --noEmit false --declaration --emitDeclarationOnly --declarationDir .output/types && tsc-alias -p tsconfig.json && npx prettier --write ./.output/"
	},
	"prettier": {
		"arrowParens": "always",
		"printWidth": 90,
		"semi": false,
		"singleQuote": true,
		"tabWidth": 2,
		"trailingComma": "none",
		"useTabs": true
	},
	"devDependencies": {
		"@babel/core": "^7.21.0",
		"@headlessui/vue": "^1.7.11",
		"@iconify/json": "^2.2.27",
		"@nuxt/devtools": "^0.2.3",
		"@nuxt/image-edge": "^1.0.0-27954023.4cee565",
		"@nuxt/test-utils-edge": "3.2.3-rc.0-27949872.7479d46",
		"@nuxtjs/eslint-config": "^12.0.0",
		"@nuxtjs/eslint-config-typescript": "^12.0.0",
		"@nuxtjs/i18n": "^8.0.0-beta.9",
		"@pinia/nuxt": "^0.4.7",
		"@tailwindcss/typography": "0.5.9",
		"@vee-validate/zod": "^4.7.4",
		"@vueuse/core": "^9.13.0",
		"@vueuse/nuxt": "^9.13.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.34.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-nuxt": "^4.0.0",
		"eslint-plugin-prettier": "^4.2.1",
		"js-yaml": "^4.1.0",
		"lint-staged": "^13.1.2",
		"nuxt": "3.2.2",
		"pinia": "^2.0.32",
		"postcss": "8.4.21",
		"postcss-loader": "^7.0.2",
		"prettier": "^2.8.4",
		"sass": "1.58.3",
		"sass-loader": "^13.2.0",
		"serve": "^14.2.0",
		"tailwindcss": "^3.2.7",
		"translate": "^1.4.1",
		"tsc-alias": "^1.8.2",
		"typescript": "^4.9.5",
		"unplugin-icons": "^0.15.3",
		"unplugin-vue-components": "^0.24.0",
		"vee-validate": "^4.5.8",
		"vite": "^4.1.4",
		"vitest": "^0.28.5",
		"vue": "^3.2.47",
		"vue-tsc": "^1.1.7",
		"webpack": "^5.75.0",
		"zod": "^3.20.6",
    "vue-toastification": "^2.0.0-rc.5"
  },
	"lint-staged": {
		"**/*.{js,ts,vue,html}": [
			"npm lint:fix"
		]
	},
	"engines": {
		"node": ">=18.12.1",
		"npm": ">=8.19.2"
	}
}
