{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "outDir": "build",
    "noImplicitAny": true,
    "strictFunctionTypes": false,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "baseUrl": ".",
    "types": [
      "webpack-env"
    ],
    "paths": {
      "@vue-ts*": ["src/*"],
      "$node/*": ["./node_modules/*"]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ],
    "typeRoots": ["node_modules/@types", "typings.d.ts"]
  },
  "include": [
    "./src/**/*"
  ],
  "files": [
    "typings.d.ts"
  ]
}
