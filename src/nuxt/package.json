    "@iconify/json": "^2.1.150",
    "@intlify/nuxt3": "^0.2.4",
    "@nuxt/content": "2.2.2",
    "@nuxt/test-utils-edge": "3.0.1-rc.0-27845213.4641e8e",
    "@nuxtjs/eslint-config": "^12.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@pinia/nuxt": "^0.4.6",
    "@vueuse/core": "^9.6.0",
    "@vueuse/nuxt": "^9.6.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "js-yaml": "^4.1.0",
    "lint-staged": "^13.1.0",
    "nuxt": "3.0.0",
    "nuxt-windicss": "^2.6.0",
    "pinia": "^2.0.28",
    "postcss": "8.4.19",
    "postcss-loader": "^7.0.2",
    "prettier": "^2.8.1",
    "sass": "1.56.2",
    "sass-loader": "^13.2.0",
    "serve": "^14.1.2",
    "three": "^0.147.0",
    "translate": "^1.4.1",
    "typescript": "^4.9.4",
    "unplugin-icons": "^0.14.14",
    "unplugin-vue-components": "^0.22.11",
    "vite": "^4.0.0",
    "vitest": "^0.25.7",
    "vue": "^3.2.45",
