{
	"name": "frontend",
	"version": "0.1.2",
	"private": true,
	"scripts": {
		"dev": "vite --host",
		"serve:vue": "vue-cli-service serve --port 5000",
		"build:vue": "vue-cli-service build",
		"watch:vue": "vue-cli-service build --watch --mode development",
		"build:vite": "vue-tsc --noEmit && vite build",
		"watch:vite": "vue-tsc --noEmit && vite build --watch --mode development",
		"dts:vite": "vue-tsc --declaration --emitDeclarationOnly && tsc-alias -p tsconfig.json && npx prettier --write ./build/",
		"lint:vue": "vue-cli-service lint",
		"lint:scss": "sass-lint -v",
		"lint:scss:fix": "sass-lint-auto-fix -c .sass-lint-auto-fix.yml --config-sass-lint .sass-lint.yml",
		"lint:prettier": "prettier --check .",
		"lint:prettier:fix": "prettier --write .",
		"lint:all": "npm run lint:vue && npm run lint:scss && npm run lint:prettier",
		"lint:all:fix": "npm run lint:vue && npm run lint:scss:fix && npm run lint:prettier:fix"
	},
	"dependencies": {
		"@apollo/client": "^3.7.1",
		"@fortawesome/fontawesome-svg-core": "^6.2.0",
		"@fortawesome/free-brands-svg-icons": "^6.2.0",
		"@fortawesome/free-solid-svg-icons": "^6.2.0",
		"@fortawesome/vue-fontawesome": "^3.0.1",
		"@popperjs/core": "^2.11.6",
		"@stripe/stripe-js": "^1.42.0",
		"@vee-validate/zod": "^4.7.1",
		"@vue/apollo-option": "^4.0.0-alpha.20",
		"@vue/cli-plugin-eslint": "^5.0.8",
		"@vueuse/components": "^9.4.0",
		"@vueuse/core": "^9.4.0",
		"axios": "^1.1.3",
		"core-js": "^3.26.0",
		"graphql": "^16.6.0",
		"graphql-tag": "^2.12.6",
		"lodash": "^4.17.21",
		"lottie-web": "^5.9.6",
		"mitt": "^3.0.0",
		"prettier": "^2.7.1",
		"swiper": "^8.4.4",
		"universal-cookie": "^4.0.4",
		"uuid": "^9.0.0",
		"vee-validate": "^4.7.1",
		"vite-plugin-environment": "^1.1.3",
		"vite-plugin-html-env": "^1.2.7",
		"vue": "^3.2.41",
		"vue-class-component": "^8.0.0-rc.1",
		"vue-horizontal": "^1.0.2",
		"vue-i18n": "^9.3.0-beta.3",
		"vue-meta": "^3.0.0-alpha.10",
		"vue-router": "^4.1.6",
		"vue-social-sharing": "^4.0.0-alpha4",
		"vue-toastification": "^2.0.0-rc.5",
		"vue3-form-validation": "^5.1.0",
		"vuex": "^4.1.0",
		"workbox-webpack-plugin": "^6.5.4",
		"zod": "^3.19.1"
	},
	"devDependencies": {
		"@babel/core": "^7.19.6",
		"@babel/eslint-parser": "^7.19.1",
		"@intlify/vite-plugin-vue-i18n": "^6.0.3",
		"@types/lodash": "^4.14.186",
		"@typescript-eslint/eslint-plugin": "^5.41.0",
		"@typescript-eslint/parser": "^5.41.0",
		"@vitejs/plugin-vue": "^3.2.0",
		"@vue/cli-plugin-babel": "^5.0.8",
		"@vue/cli-plugin-pwa": "~5.0.8",
		"@vue/cli-service": "^5.0.8",
		"@vue/compiler-sfc": "^3.2.41",
		"@vue/eslint-config-typescript": "^11.0.2",
		"eslint": "^8.26.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-vue": "^9.6.0",
		"sass": "^1.55.0",
		"sass-lint": "^1.13.1",
		"sass-lint-auto-fix": "^0.21.2",
		"sass-loader": "^13.1.0",
		"tsc-alias": "^1.7.0",
		"typescript": "^4.8.4",
		"vite": "^3.2.1",
		"vue-eslint-parser": "^9.1.0",
		"vue-stripe-js": "^1.0.1",
		"vue-tsc": "^1.0.9",
		"vuex-module-decorators": "^2.0.0"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/vue3-recommended"
		],
		"parser": "vue-eslint-parser",
		"parserOptions": {
			"parser": "@typescript-eslint/parser",
			"sourceType": "module"
		},
		"rules": {}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead"
	]
}
