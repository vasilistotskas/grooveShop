    "@fortawesome/vue-fontawesome": "^3.0.1",
    "@popperjs/core": "^2.11.6",
    "@stripe/stripe-js": "^1.38.1",
    "@vee-validate/zod": "^4.6.10",
    "@vue/apollo-option": "^4.0.0-alpha.20",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vueuse/components": "^9.3.0",
    "@vueuse/core": "^9.3.0",
    "axios": "^0.27.2",
    "core-js": "^3.25.3",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "lottie-web": "^5.9.6",
    "mitt": "^3.0.0",
    "prettier": "^2.7.1",
    "sass-lint": "^1.13.1",
    "swiper": "^8.4.2",
    "universal-cookie": "^4.0.4",
    "uuid": "^9.0.0",
    "vee-validate": "^4.6.10",
    "vite-plugin-environment": "^1.1.2",
    "vite-plugin-html-env": "^1.2.7",
    "vue": "^3.2.40",
    "vue-class-component": "^8.0.0-rc.1",
    "vue-horizontal": "^1.0.2",
    "vue-i18n": "^9.3.0-beta.3",
    "vue-meta": "^3.0.0-alpha.10",
    "vue-router": "^4.1.5",
    "vue-social-sharing": "^4.0.0-alpha4",
    "vue-toastification": "^2.0.0-rc.5",
    "vue3-form-validation": "^5.1.0",
